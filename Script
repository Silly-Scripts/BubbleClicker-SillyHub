if game.PlaceId == 11746859781 then 
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Silly Hub", HidePremium = false, IntroText = "Silly Hub", SaveConfig = true, ConfigFolder = "OrionTest"})

-- Values 

_G.AutoTap = true
_G.AutoEgg = true
_G.AutoWorld = true
_G.AutoGift = true
_G.AutoCode = true
-- Functions

function DoTap()
    spawn (function()
        while _G.AutoTap == true do
            local args = {[1] = {[1] = {[1] = false}, [2] = {[1] = 2 } }}
            game:GetService("ReplicatedStorage").Remotes:FindFirstChild("blow bubble"):FireServer(unpack(args))
         wait(0.1)
        end
    end)
 end

function CommonEgg()
    spawn (function()
        while _G.AutoEgg == true do
local args = {[1] = {[1] = {[1] = "Common Egg" },[2] = { [1] = false}}}game:GetService("ReplicatedStorage").Remotes:FindFirstChild("buy egg"):InvokeServer(unpack(args))
         wait(0.5)
        end
    end)
 end

